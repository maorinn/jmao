package com;

public class tese1 {
public static void main(String[] args) {
	int i = 0,j = 0,t;
	StringBuffer a = new StringBuffer();
	String[] s = new String[100];
	String c = "";
	int[] arr = new int[100];
	for(i=0;i<100;i++){
		//arr[i] = i+1;
		a.append(i+1+",");
	}
	
c = a.substring(0);
s = c.split(",");
while (s.length>0) {
	for(i=1;i<s.length;i++){
		if(i%3 == 0){
			s[i-1] = "";
		}
		//a.deleteCharAt(i);

	}
}

for(i=0;i<s.length;i++){
	System.out.print(s[i]);
}



}
}
