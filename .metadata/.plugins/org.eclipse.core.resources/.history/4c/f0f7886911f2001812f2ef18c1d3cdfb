package com;

public class a {
public static void main(String[] args) {
	String s = "113@ ere qqq yyui";
	String[] ss = s.split(" ");
	System.out.println(ss);
	int i;
	for(i =0;i<s.length();i++){
		if(s.charAt(i)>='a'&&s.charAt(i)<='z'||s.charAt(i)<='A'&&s.charAt(i)>='Z'){
			System.out.print(s.charAt(i));
		}
	}
}
}
