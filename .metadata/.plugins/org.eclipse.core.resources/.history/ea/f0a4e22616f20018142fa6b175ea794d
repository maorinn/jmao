package com;

public class a {
public static void main(String[] args) {
	String s = "113@ ere qqq yyui";
	String s1= s.replaceAll("@", "");
	String[] ss = s1.split(" ");
	int i;
	for(i =0;i<ss.length;i++){
		System.out.println(ss[i]);
	}
	String c = "To be or not to be";
	String[] c1 = c.split("");
	System.out.println(c1[0]);
	
}
}
