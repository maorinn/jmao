package com;

public class MyRunnable implements Runnable {
	int p = 100;
	public void run() {
		as();
	}
	public void as(){
		
		while(p>1){
			p--;
			System.out.println(Thread.currentThread().getName()+"Âô"+p);
			try {
				Thread.sleep(500);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}

		if (p==0) {
			System.out.println("Âô¹âÁË");
		}
	}
	

}

