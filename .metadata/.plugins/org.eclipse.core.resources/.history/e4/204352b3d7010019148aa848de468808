	@Override
	public synchronized void run() {

		
	}

}

class m{
	int p = 100;
	while(p>1){
		p--;
		System.out.println(Thread.currentThread().getName()+"Âô"+p);
	}
	try {
		Thread.sleep(20000);
	} catch (InterruptedException e) {
