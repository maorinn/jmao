package com;

public class TestRunnable {
int p = 100;
int i;
public static void main(String[] args) {
	MyRunnable r1 = new MyRunnable();
	Thread t1 = new Thread(r1,"窗口一");
	Thread t2 = new Thread(r1,"窗口二");
	Thread t3 = new Thread(r1,"窗口三");
	Thread t4 = new Thread(r1,"窗口四");
	for(int i = 1;i<=100;i++)
		t1.start();
		t2.start();
		t3.start();
		t4.start();
	

}
}
