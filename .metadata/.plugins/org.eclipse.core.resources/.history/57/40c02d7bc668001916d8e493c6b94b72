package com.Find;

public class Find {
	public int Half(int[] arr,int val) {
		int star = 0;
		int end = arr.length-1;
		int min;
		int re = -1;
		while (star<=end) {
			min = (star+end)/2;
			System.out.println(arr[min]);
			if (val!=arr[min]) {
				if (val>arr[min]) {
					star = min+1;
				}else if (val<arr[min]) {
					end = min-1;
				}
			}else {
				re = min;
				break;
			}
		}
			return re;
		}
}
