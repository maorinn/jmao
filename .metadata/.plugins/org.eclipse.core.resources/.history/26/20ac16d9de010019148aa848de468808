package com;

public class MyRunnable implements Runnable {
	int p = 0;
	public void run() {
		as();
	}
	public  void as(){
		
			for(int i = 0;i<=p;i++){
				p++;
				System.out.println(Thread.currentThread().getName()+"¬Ù"+p);
				if(p>100){
					System.out.println("¬Ùπ‚¡À");
					break;
				}
				try {
					Thread.sleep(100);
				} catch (InterruptedException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
			}
		}


			
		}

